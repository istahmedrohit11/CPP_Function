#include<iostream>
using namespace std;
int return_sum(int a,int b){
    int s=a+b;
    return s;
}
void dont_ruturn_sum(int a,int b){
    int s=a+b;
    cout<<"Using_void_sum is: "<<s<<"\n";
}
int main(){
    int a,b;
    cout<<"Enter two Number: ";
    cin>>a>>b;
    cout<<"Using_int_Sum is:"<<return_sum(a,b)<<"\n";
    dont_ruturn_sum(a,b);
    return 0;
}
